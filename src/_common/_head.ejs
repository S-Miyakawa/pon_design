<meta charset="UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<!-- ogp -->
<meta property="og:type" content="website" />
<%_ if(meta.title) { _%>
<title><%= meta.title%></title>
<%_ }else{ _%>
<title>PON DESIGN</title>
<%_ } _%>
<meta name="description" content="<%= meta.description %>" />
<meta property="og:title" content="<%= meta.title%>" />
<meta property="og:description" content="<%= meta.description %>" />
<%_ if(meta.url) { _%>
<meta property="og:url" content="https://traveler20.site/<%= meta.url %>" />
<%_ }else{ _%>
<meta property="og:url" content="https://traveler20.site/" />
<%_ } _%>
<!-- ogp-image -->
<%_ if(meta.ogp) { _%>
<meta property="og:image" content="https://traveler20.site/<%= meta.ogp %>" />
<%_ }else{ _%>
<meta
    property="og:image"
    content="https://traveler20.site/asset/img/ogp.webp"
/>
<%_ } _%>
<meta property="og:locale" content="ja_JP" />
<meta name="twitter:card" content="summary_large_image" />
<meta name="twitter:site" content="@traveler20site" />
<link rel="icon" type="image/x-icon" href="<%= path%>asset/img/favicon.ico" />
<!-- css -->
<link rel="stylesheet" href="<%= path%>asset/css/lib/swiper-bundle.min.css">
<link rel="stylesheet" href="<%= path%>asset/css/style.css" />

<!-- google fonts -->
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@400;700&family=Noto+Sans+JP:wght@300;400;500;700&display=swap" rel="stylesheet">

<!-- js -->
<script src="<%= path%>asset/js/jquery-3.6.4.min.js" defer></script>
<script src="<%= path%>asset/js/swiper-bundle.min.js" defer></script>
<script src="<%= path%>asset/js/script.js" defer></script>
